<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/commit_detail_nav_graph"
    app:startDestination="@id/commitDetailFragment">

    <fragment
        android:id="@+id/commitDetailFragment"
        android:name="com.nimroddayan.commitbrowser.home.commitdetail.CommitDetailFragment"
        android:label="{message}"
        tools:layout="@layout/commit_detail_fragment">
        <argument
            android:name="sha"
            app:argType="string" />
        <argument
            android:name="message"
            app:argType="string" />
    </fragment>

</navigation>
